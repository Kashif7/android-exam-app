<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kashif.examapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/examapp_logo"
        android:label="Quizzer"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ExamActivity" />
        <activity android:name=".TeacherActivity" />
        <activity android:name=".ExamResultActivity" />
        <activity android:name=".ExamResultDetailActivity" />
        <activity android:name=".LoginActivity"/>
    </application>

</manifest>